void R_40()							// РЕЖИМ 40 "Включить нагрев канала 1"
{
	// выставление параметра канала
	ObjBPN[0]->parBPN = par_n[0];

	// запуск режима
	BPNOn(ObjBPN[0]);				// включение нагерва канала 1
}
