		shr[9] = 8;				// переход на шаг 8_1
		sh_	=1;
		goto L8_1;
	}
	goto L101;
L8:	if(!(zin[4]&0x4000))			// проверка нал. пл. перед кам.
	{
		diagn[8] |= 0x01;			// отказ: "Ман. не взял пластину из кассеты"
		goto L101;		
