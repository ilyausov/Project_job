    };
    if ( shr[61] )              // Проверка
    {                           // активации
        R_61();                 // режима 61 "Откл. нагрева канала 12"
    };
    if ( shr[62] )              // Проверка
    {                           // активации
        R_62();                 // режима 62 "Вкл. нагрева канала 13"
    };
    if ( shr[63] )              // Проверка
