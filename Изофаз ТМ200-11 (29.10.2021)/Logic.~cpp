	diagn[8] &= (~0x01);			// сброс диагностик МАН перемещения
L8_1:N_PL++;
	shr[9]=11;
	sh_=0;
	goto L101;
L9: sh_=0;
	goto L101;
L10: sh_=0;
	goto L101;
L11: sh_=0;
	goto L101;
