	goto L100;
	
L1: VBPN();							// включить БПН
	if(sh_) goto L101;
	if(par_n[0])					// Есть задание на канал 1
	{
		if(!shr[40])				// Есть активизация режима(вкл. нагр. кан. 1)
		{
			sh[40]  = 1;			// активация режима 40
			shr[40] = 1;
		}
		shr[33] = 2;	
