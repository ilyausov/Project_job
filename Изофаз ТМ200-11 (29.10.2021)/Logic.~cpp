    };
	if ( shr[25] )              // Проверка
    {                           // активации
        R_25();                 // режима 25 "РРГ6"
    };
	if ( shr[26] )              // Проверка
    {                           // активации
        R_26();                 // режима 26 "РРГ7"
    };
	if ( shr[29] )              // Проверка
    {                           // активации
